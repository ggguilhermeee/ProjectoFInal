<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
             xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:tools="http://schemas.android.com/tools"
             android:layout_width="match_parent"
             android:layout_height="match_parent"
             tools:context="com.silent_manager.g29.silent_manager.android_components.fragments.LogInFragment">

    <android.support.constraint.ConstraintLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent" android:id="@+id/logIn_containner">

        <TextView
                android:text="@string/login_title_label"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/log_in_title_txt" app:layout_constraintEnd_toEndOf="parent"
                android:layout_marginEnd="8dp"
                android:layout_marginRight="8dp" app:layout_constraintStart_toStartOf="parent"
                android:layout_marginLeft="8dp" android:layout_marginStart="8dp" android:layout_marginTop="11dp"
                app:layout_constraintTop_toTopOf="parent" android:layout_marginBottom="8dp"
                app:layout_constraintBottom_toBottomOf="parent" android:textSize="18sp"
                app:layout_constraintHorizontal_bias="0.46" app:layout_constraintVertical_bias="0.04"/>
        <TextView
                android:text="@string/login_email_label"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/log_in_email_txt" app:layout_constraintEnd_toEndOf="parent"
                android:layout_marginEnd="8dp" android:layout_marginRight="8dp"
                app:layout_constraintStart_toStartOf="parent" android:layout_marginLeft="8dp"
                android:layout_marginStart="8dp" android:layout_marginTop="8dp"
                app:layout_constraintTop_toBottomOf="@+id/log_in_title_txt"
                app:layout_constraintHorizontal_bias="0.0"
                android:layout_marginBottom="8dp" app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintVertical_bias="0.043"/>
        <TextView
                android:text="@string/login_password_label"
                android:layout_width="wrap_content"
                android:layout_height="17dp"
                android:id="@+id/log_in_password_txt" app:layout_constraintEnd_toEndOf="parent"
                android:layout_marginEnd="8dp" android:layout_marginRight="8dp"
                app:layout_constraintStart_toStartOf="parent" android:layout_marginLeft="8dp"
                android:layout_marginStart="8dp"
                app:layout_constraintHorizontal_bias="0.0"
                android:layout_marginTop="8dp" app:layout_constraintTop_toBottomOf="@+id/log_in_email_value"
                android:layout_marginBottom="8dp" app:layout_constraintBottom_toTopOf="@+id/log_in_password_value"
                app:layout_constraintVertical_bias="0.722"/>
        <EditText
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:inputType="textPersonName"
                android:ems="10"
                android:id="@+id/log_in_email_value"
                android:layout_marginBottom="8dp"
                app:layout_constraintBottom_toBottomOf="parent"
                android:layout_marginEnd="40dp" app:layout_constraintEnd_toEndOf="parent"
                android:layout_marginRight="40dp"
                android:hint="@string/login_email_hint_label" android:layout_marginStart="8dp"
                app:layout_constraintStart_toStartOf="parent" android:layout_marginLeft="8dp"
                android:layout_marginTop="8dp" app:layout_constraintTop_toBottomOf="@+id/log_in_email_txt"
                app:layout_constraintHorizontal_bias="0.446" app:layout_constraintVertical_bias="0.017"
                android:autofillHints=""/>
        <EditText
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:inputType="textPassword"
                android:ems="10"
                android:id="@+id/log_in_password_value"
                android:layout_marginBottom="256dp"
                app:layout_constraintBottom_toBottomOf="parent" android:layout_marginEnd="40dp"
                app:layout_constraintEnd_toEndOf="parent" android:layout_marginRight="40dp"
                android:hint="@string/login_password_hint_label"
                android:layout_marginStart="8dp"
                app:layout_constraintStart_toStartOf="parent" android:layout_marginLeft="8dp"
                app:layout_constraintHorizontal_bias="0.0"
                android:layout_marginTop="8dp" app:layout_constraintTop_toBottomOf="@+id/log_in_email_value"
                app:layout_constraintVertical_bias="1.0" android:autofillHints=""/>
        <Button
                android:text="@string/login_sign_in_btm"
                android:layout_width="0dp"
                android:layout_height="44dp"
                android:id="@+id/login_sign_in_btm" app:layout_constraintEnd_toEndOf="parent"
                android:layout_marginEnd="13dp"
                android:layout_marginRight="13dp" app:layout_constraintStart_toStartOf="parent"
                android:layout_marginLeft="8dp" android:layout_marginStart="8dp" android:layout_marginTop="49dp"
                app:layout_constraintTop_toBottomOf="@+id/log_in_password_value" android:layout_marginBottom="8dp"
                app:layout_constraintBottom_toBottomOf="parent" app:layout_constraintVertical_bias="0.0"
                app:layout_constraintHorizontal_bias="0.0"/>
        <TextView
                android:text="@string/login_forget_password_txt"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/login_forget_my_password_txt" app:layout_constraintEnd_toEndOf="parent"
                android:layout_marginEnd="8dp"
                android:layout_marginRight="8dp" app:layout_constraintStart_toStartOf="parent"
                android:layout_marginLeft="8dp" android:layout_marginStart="8dp" android:layout_marginTop="6dp"
                app:layout_constraintTop_toBottomOf="@+id/login_sign_in_btm"
                app:layout_constraintHorizontal_bias="0.502" android:layout_marginBottom="8dp"
                app:layout_constraintBottom_toBottomOf="parent" app:layout_constraintVertical_bias="0.0"/>
        <Button
                android:text="@string/login_create_account_btm"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/login_create_accout_btm" app:layout_constraintEnd_toEndOf="parent"
                android:layout_marginEnd="8dp"
                android:layout_marginRight="8dp" app:layout_constraintStart_toStartOf="parent"
                android:layout_marginLeft="8dp" android:layout_marginStart="8dp" android:layout_marginTop="9dp"
                app:layout_constraintTop_toBottomOf="@+id/login_forget_my_password_txt"
                android:layout_marginBottom="8dp" app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintVertical_bias="0.211"/>
    </android.support.constraint.ConstraintLayout>
</FrameLayout>
